{
  "version": 2,
  "builds": [
    {
      "src": "src/app/server.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/style.css",
      "dest": "/web/style.css"
    },
    {
      "src": "/enhancements.css",
      "dest": "/web/enhancements.css"
    },
    {
      "src": "/app.js",
      "dest": "/web/app.js"
    },
    {
      "src": "/login.html",
      "dest": "/web/login.html"
    },
    {
      "src": "/landing.html",
      "dest": "/web/landing.html"
    },
    {
      "src": "/index.html",
      "dest": "/web/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/app/server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/app/server.py"
    }
  ]
}